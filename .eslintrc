{
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
        "eslint-disable-next-line": "off",
        "no-console": "off",
        "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
}